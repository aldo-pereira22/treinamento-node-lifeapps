<h1>Usuários Cadastrados: </h1>
<div class="container">
    {{#each users}}
        <div class="item">
            <p> Nome: {{this.name}} </p>
            <p> Profissão: {{this.ocupation}} </p>
            <div class="actions">
                <a href="/users/{{this.id}}">Ver detalhes</a>
            </div>
        </div>
    {{/each}}
</div>